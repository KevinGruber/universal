  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "test:server": "cd src/server && ts-mocha  -r tsconfig-paths/register **/*.spec.ts",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "start": "npm-run-all --parallel serve:server serve",
    "serve": "ng serve",
    "serve:server": "cd src/server && ts-node-dev -r tsconfig-paths/register proxy --respawn --env=prod",
    "serve:ssr": "node dist/server.js",
    "build": "rimraf dist && yarn run build:client && yarn run build:server",
    "build:client": "ng build --prod",
    "build:server": "ng run ng-shop:server:production && yarn run build:ssr",
    "build:ssr": "cd src/server && webpack --config webpack.config.js"
  },
  "private": true,
